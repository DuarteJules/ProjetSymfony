{% extends 'base.html.twig' %}


{% block title %}Job List{% endblock %}

{% block body %}

    <div class="container p-2">
        <h1>Liste des candidatures de {{candidat.name}}</h1>
        <ul class="list-group">
            {% for candidatureWait in candidatureWaits %}

                     <a href="{{ path('details_job', {id: candidatureWait.id}) }}"> <li class="list-group-item">{{ candidatureWait.job.name }} </li></a>

            {% endfor %}
        </ul>
    </div>
{% endblock %}